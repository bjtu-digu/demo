<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.Hibernate.bar" table="bar"/>
  
   <id name="bar_id" column="bar_id">
            <generator class="native"/>  
       </id>

       <property name="user_id" column="user_id"/>
       <property name="topic" column="topic"/>
       <property name="exist" column="exist"/>
       <property name="bar_name" column="bar_name"/>
       <property name="create_time" column="create_time"/>
       <property name="bar_head" column="bar_head"/>
      
</hibernate-mapping>
