<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
     <class name="Hibernate.chat" table="chat">
 <composite-id >              
  <key-property name="msg_id" column="msg_id"  ></key-property>
  <key-property name="user_id" column="user_id" ></key-property>  
  <key-property name="bar_id" column="bar_id" ></key-property> 
     </composite-id>
     <property column="msg" name="msg"/>
  <property column="Date" name="date"/>
  <property column="exist" name="exist"/>
    </class>
</hibernate-mapping>
