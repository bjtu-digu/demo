<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Hibernate.bar" table="bar">
        <id column="bar_id" name="bar_id">
            <generator class="increment"/>
        </id>
        <property column="user_id" name="user_id"/>
        <property column="topic" name="topic"/>
        <property column="exist" name="exist"/>
        <property column="bar_name" name="bar_name"/>
        <property column="create_time" name="create_time"/>
        <property column="bar_head" name="bar_head"/>
        <property column="available" name="available"/>
    </class>
</hibernate-mapping>
