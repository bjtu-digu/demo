<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="Hibernate.post" table="post">
   <id column="post_id" name="post_id">
    <generator class="increment"/>
  </id>
  <property column="last_date" name="last_date"/>
  <property column="bar_id" name="bar_id"/>
  <property column="user_id" name="user_id"/>
  <property column="post_name" name="post_name"/>
  <property column="post_msg" name="post_msg"/>
  <property column="post_date" name="post_date"/>
  <property column="last_reply" name="last_reply"/>
  <property column="exist" name="exist"/>
  <property column="available" name="available"/>
</class>
</hibernate-mapping>
