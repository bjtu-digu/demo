<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="Hibernate.pic" table="pic">
      
  <id column="pic_id" name="pic_id">
    <generator class="increment"/>
  </id>
  <property column="pic_type" name="pic_type"/>
  <property column="pic_name" name="pic_name"/>
  <property column="pic_date" name="pic_date"/>
  <property column="pic_size" name="pic_size"/>
  <property column="pic_path" name="pic_path"/>
  <property column="dcp" name="dcp"/>
   <property column="exist" name="exist"/>
   
  </class>
</hibernate-mapping>
